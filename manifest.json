{
  "manifest_version": 2,
  "name": "Say NO TO SHOPPING",
  "description": "This extension is the block shopping experience",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://www.asos.com/*",
    "https://www.asos.com/*",
    "http://www.asos.com/women/"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [ "script/shopblock.js"]
    }
  ]
}